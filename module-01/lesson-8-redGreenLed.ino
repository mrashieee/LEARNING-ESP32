
int redLed = 12;
int greenLed = 14;
int delayRed = 1000;
int delayGreen = 300;
int delayLong = 1500;

void setup() {
  // put your setup code here, to run once:
  pinMode(redLed, OUTPUT);
  pinMode(greenLed, OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(greenLed, HIGH);
  delay(delayGreen);
  digitalWrite(greenLed, LOW);
  delay(delayGreen);
  digitalWrite(greenLed, HIGH);
  delay(delayGreen);
  digitalWrite(greenLed, LOW);
  delay(delayGreen);
  digitalWrite(greenLed, HIGH);
  delay(delayGreen);
  digitalWrite(greenLed, LOW);
  delay(delayGreen);
  digitalWrite(greenLed, HIGH);
  delay(delayGreen);
  digitalWrite(greenLed, LOW);
  delay(delayGreen);
  digitalWrite(greenLed, HIGH);
  delay(delayGreen);
  digitalWrite(greenLed, LOW);
  delay(delayGreen);

  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);
  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);
  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);
  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);
  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);
  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);
  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);
  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);
  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);
  digitalWrite(redLed, HIGH);
  delay(delayRed);
  digitalWrite(redLed, LOW);
  delay(delayRed);

  delay(delayLong);

}
